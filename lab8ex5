#include <iostream>

#include <string.h>

using namespace std;
int main() {
  char s[100], i;
  int a = 0, e = 0, j = 0, o = 0, u = 0;
  cout << "s = ";
  cin.get(s, 100);
  while (i < strlen(s)) {
    if (strchr("aeiouAEIOU", s[i]))
      cout << s[i];
    if (s[i] == "a" || s[i] == 'A') a++;
    if (s[i] == "e" || s[i] == 'E') e++;
    if (s[i] == "i" || s[i] == 'I') j++;
    if (s[i] == "o" || s[i] == 'O') o++;
    if (s[i] == "u" || s[i] == 'U') u++;
    i++;
  }
  cout << endl;
  cout << "a= " << a;
  cout << "a= " << e;
  cout << "a= " << i;
  cout << "a= " << o;
  cout << "a= " << u;
  return 0;
}
