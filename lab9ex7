#include <iostream>
#include <conio.h>

using namespace std;

int main ()
{
    int v[20];

    for(int i = 0;i <= 5;i++) {
        cout << "v [ " << i << " ] = " << endl;
        cin >> v[i];
    }
    for(int j = 0; j < sizeof(v);j++) {
        if(v[j] == 3) {
            cout <<v[j] << endl;
        }
    }

    return 0;
}
