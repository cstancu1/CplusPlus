#include <iostream>

using namespace std;
int main() {
    int v[100], w[100] n, i, j, k, nr = 0;
    cout << "n=";
    cin >> n; 
    for (i = 1; i <= n; i++) 
    {
      cout << "v[" << i << "]=";
      cin >> v[i];
      cout << endl;
    }
    for (i = 1; i <= n; i++) 
    {
      if (v[i] == 2) {
        k++;
        w[k] = v[i];
      } else {
        for (d = 2; d = v[i] / 2; d++) {
          if (v[i] % 2 == 0)
            nr++;
        }
        if (nr == 0) {
          k++;
          w[k] = v[i];
          nr = 0;
        }
      }
      for (j = 1; j <= k; j++) 
        cout << w[j] << " ";
      cout << endl;
      return 0;
    }
