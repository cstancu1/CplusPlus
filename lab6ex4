#include <iostream>

using namespace std;
int main() {
    int a[100][100], i, j, nl, nc, max, min;
    cout << "nl=";
    cin >> nl; 
    cout << "nc=";
    cin >> nc;
    for (i = 1; i <= nl; i++)
      for (j = 1; j <= nc; j++) {
        cout << "a[" << i << "][" << j << "]=";
        cin >> a[i][j];
      }
}
