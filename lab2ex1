#include <iostream>

using namespace std;

int main()
{
    int a,b,c,d, maxim;
    cout<<"a=";cin>>a;
    cout<<"b=";cin>>b;
    cout<<"c=";cin>>c;
    cout<<"d=";cin>>d;
    if(a>b && a>c && a>d){
        maxim = a;
    }
    else if(b>a && b>c && b>d){
        maxim = b;
    }
    else if(c>a && c>b && c>d){
        maxim = c;
    }
    else{
        maxim = d;
    }
    cout<<maxim;
}
